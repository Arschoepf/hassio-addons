{
  "name": "Loki",
  "url": "https://grafana.com/oss/loki/",
  "version": "0.1",
  "slug": "loki",
  "ingress": false,
  "ingress_port": 0,
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "description": "Loki for Home Assistant",
  "startup": "system",
  "boot": "manual",
  "map": ["share", "ssl"],
  "ports": {
    "3100/tcp": null
  },
  "ports_description": {
    "3100/tcp": "HTTP listen port"
  },
  "options": {
    "ssl": false,
    "log_level": "info"
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str?",
    "keyfile": "str?",
    "cafile": "str?",
    "config_path": "str?",
    "log_level": "list(debug|info|warning|error)?"
  }
}
